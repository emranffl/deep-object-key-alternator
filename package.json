{
  "name": "deep-object-key-alternator",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "LICENSE"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "branches": [
      {
        "name": "main"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/emranffl/deep-object-key-alternator"
  },
  "scripts": {
    "build": "rm -r dist --force && tsc",
    "commit": "git-cz",
    "docify": "npx typedoc --plugin typedoc-plugin-markdown --out docs src/index.ts --hideBreadcrumbs --hideInPageTOC && rm -r docs/README.md",
    "prepublishOnly": "npm run build",
    "semantic-release": "semantic-release --branches main",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "deep",
    "object",
    "key",
    "alternator",
    "deep-object-key-alternator",
    "deep-object-key-alternate",
    "object-key-alternator",
    "object-key-alternate"
  ],
  "author": "Emran Hossain",
  "license": "MIT",
  "dependencies": {
    "commitizen": "^4.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.3",
    "cz-conventional-changelog": "^3.3.0",
    "semantic-release": "^22.0.5",
    "ts-node": "^10.9.1",
    "typedoc": "^0.25.2",
    "typedoc-plugin-markdown": "^3.16.0",
    "typescript": "^5.2.2"
  }
}
